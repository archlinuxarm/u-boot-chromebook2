/ {
	aliases {
		console = "/serial@12C30000";
		serial0 = "/serial@12C30000";
		sdmmc0 = "/mshci@12200000";
		sdmmc1 = "/mshci@12220000";
		spi0 = "/spi@12d20000";
		spi1 = "/spi@12d30000";
		spi2 = "/spi@12d40000";
		spi3 = "/spi@131a0000";
		spi4 = "/spi@131b0000";
		i2c0 = "/i2c@12c60000";
		i2c1 = "/i2c@12c70000";
		i2c2 = "/i2c@12c80000";
		i2c3 = "/i2c@12c90000";
		i2c4 = "/i2c@12ca0000";
		i2c5 = "/i2c@12cb0000";
		i2c6 = "/i2c@12cc0000";
		i2c7 = "/i2c@12cd0000";
	};

	tmu@10060000 {
		samsung,mux		= <6>;
		samsung,min-temp	= <25>;
		samsung,max-temp	= <125>;
		samsung,start-warning	= <95>;
		samsung,start-tripping	= <105>;
		samsung,hw-tripping	= <110>;
		samsung,efuse-min-value	= <40>;
		samsung,efuse-value	= <55>;
		samsung,efuse-max-value	= <100>;
		samsung,slope		= <268470274>;
		samsung,dc-value	= <25>;
	};

	serial@12C30000 {
		id = <3>;
	};

	config {
		google,bad-wake-gpios = <&gpio 0x70 0>;
		hwid = "DAISY TEST A-A 9382";
		machine-arch-id = <3774>;
	};

	chromeos-config {
		virtual-dev-switch;

		vboot-flag-write-protect {
			compatible = "google,gpio-flag";
			gpio = <&gpio 102 1>;
		};
		vboot-flag-developer {
			compatible = "google,const-flag";
			value = <0>;
		};
		vboot-flag-lid-open {
			compatible = "google,gpio-flag";
			gpio = <&gpio 189 0>;
		};
		vboot-flag-power-off {
			compatible = "google,gpio-flag";
			gpio = <&gpio 171 1>;
		};
		vboot-flag-oprom-loaded {
			compatible = "google,const-flag";
			value = <0>;
		};
		vboot-flag-ec-in-rw {
			compatible = "google,gpio-flag";
			gpio = <&gpio 103 0>;
		};
	};

	memory {
		reg = <0x40000000 0x80000000>;
	};

	ramoops {
		reg = <0x41f00000 0x00100000>;
	};

	sdhci@12200000 {
		status = "disabled";
	};

	sdhci@12210000 {
		status = "disabled";
	};

	sdhci@12220000 {
		samsung,sdhci-bus-width = <4>;
	};

	sdhci@12230000 {
		status = "disabled";
	};

        mmc@12200000 {
                samsung,bus-width = <8>;
                samsung,timing = <1 3 3>;
                samsung,removable = <0>;
                enable-gpios = <&gpio 58 0>;
        };

        mmc@12210000 {
                status = "disabled";
        };

        mmc@12220000 {
                samsung,bus-width = <4>;
                samsung,timing = <1 2 3>;
                samsung,removable = <1>;
        };

        mmc@12230000 {
                status = "disabled";
        };


	dmc@10C00000 {
		mem-interleave-size = <0x1f>;
		mem-type = "ddr3";
		clock-frequency = <800000000>;
		arm-frequency = <1700000000>;
	};

	i2c@12cd0000 {
		display@50 {
			reg = <0x50>;
			compatible = "samsung,exynos-lcd";
		};
	};

	spi@12d30000 {
		/*
		 * 50MHz seems to be unreliable on DVT machines; so fall back
		 * to 45MHz for now.
		 */
		spi-max-frequency = <45000000>;
		firmware_storage_spi: flash@0 {
			bus = <1>;
			spi-max-frequency = <45000000>;
		};
	};

	sound@12d60000 {
		samsung,i2s-epll-clock-frequency = <192000000>;
		samsung,i2s-sampling-rate = <48000>;
		samsung,i2s-bits-per-sample = <16>;
		samsung,i2s-channels = <2>;
		samsung,i2s-lr-clk-framesize = <256>;
		samsung,i2s-bit-clk-framesize = <32>;

		codec-enable-gpio = <&gpio 0xb7 0>;
	};

	ehci@12110000 {
		compatible = "samsung,exynos-ehci", "usb-ehci";
		reg = <0x12110000 0x100>;
	};

	cros-ec-keyb {
		compatible = "google,cros-ec-keyb";
		google,key-rows = <8>;
		google,key-columns = <13>;
		google,repeat-delay-ms = <240>;
		google,repeat-rate-ms = <30>;
		google,ghost-filter;
		/*
		 * Keymap entries take the form of 0xRRCCKKKK where
		 * RR=Row CC=Column KKKK=Key Code
		 * The values below are for a US keyboard layout and
		 * are taken from the Linux driver. Note that the
		 * 102ND key is not used for US keyboards.
		 */
		linux,keymap = <
			/* CAPSLCK F1         B          F10     */
			0x0001003a 0x0002003b 0x00030030 0x00040044
			/* N       =          R_ALT      ESC     */
			0x00060031 0x0008000d 0x000a0064 0x01010001
			/* F4      G          F7         H       */
			0x0102003e 0x01030022 0x01040041 0x01060023
			/* '       F9         BKSPACE    L_CTRL  */
			0x01080028 0x01090043 0x010b000e 0x0200001d
			/* TAB     F3         T          F6      */
			0x0201000f 0x0202003d 0x02030014 0x02040040
			/* ]       Y          102ND      [       */
			0x0205001b 0x02060015 0x02070056 0x0208001a
			/* F8      GRAVE      F2         5       */
			0x02090042 0x03010029 0x0302003c 0x03030006
			/* F5      6          -          \       */
			0x0304003f 0x03060007 0x0308000c 0x030b002b
			/* R_CTRL  A          D          F       */
			0x04000061 0x0401001e 0x04020020 0x04030021
			/* S       K          J          ;       */
			0x0404001f 0x04050025 0x04060024 0x04080027
			/* L       ENTER      Z          C       */
			0x04090026 0x040b001c 0x0501002c 0x0502002e
			/* V       X          ,          M       */
			0x0503002f 0x0504002d 0x05050033 0x05060032
			/* L_SHIFT /          .          SPACE   */
			0x0507002a 0x05080035 0x05090034 0x050B0039
			/* 1       3          4          2       */
			0x06010002 0x06020004 0x06030005 0x06040003
			/* 8       7          0          9       */
			0x06050009 0x06060008 0x0608000b 0x0609000a
			/* L_ALT   DOWN       RIGHT      Q       */
			0x060a0038 0x060b006c 0x060c006a 0x07010010
			/* E       R          W          I       */
			0x07020012 0x07030013 0x07040011 0x07050017
			/* U       R_SHIFT    P          O       */
			0x07060016 0x07070036 0x07080019 0x07090018
			/* UP      LEFT    */
			0x070b0067 0x070c0069>;
	};
};
